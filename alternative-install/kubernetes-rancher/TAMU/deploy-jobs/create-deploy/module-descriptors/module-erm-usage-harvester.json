{
  "id" : "mod-erm-usage-harvester-1.0.0",
  "name" : "erm-usage-harvester",
  "requires" : [ {
    "id" : "users-bl",
    "version" : "3.3"
  }, {
    "id" : "usage-data-providers",
    "version" : "1.0"
  }, {
    "id" : "aggregator-settings",
    "version" : "1.0"
  }, {
    "id" : "counter-reports",
    "version" : "1.0"
  } ],
  "provides" : [ {
    "id" : "usage-harvester",
    "version" : "1.0",
    "handlers" : [ {
      "methods" : [ "GET" ],
      "pathPattern" : "/harvester/start"
    } ]
  } ],
  "launchDescriptor" : {
    "dockerImage" : "folioorg/mod-erm-usage-harvester:1.0.0",
    "dockerPull" : true,
    "env" : [ {
      "name" : "JAVA_OPTIONS",
      "value" : "-Xmx256m"
    } ],
    "dockerArgs" : {
      "HostConfig" : {
        "PortBindings" : {
          "8081/tcp" : [ {
            "HostPort" : "%p"
          } ]
        }
      }
    }
  }
}