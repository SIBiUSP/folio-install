{
  "id" : "mod-licenses-1.0.2",
  "name" : "mod-licenses",
  "provides" : [ {
    "id" : "licenses",
    "version" : "1.0",
    "handlers" : [ {
      "methods" : [ "GET", "POST" ],
      "pathPattern" : "/licenses/*"
    } ]
  }, {
    "id" : "_tenant",
    "version" : "1.0",
    "interfaceType" : "system",
    "handlers" : [ {
      "methods" : [ "GET" ],
      "pathPattern" : "/_/tenant"
    }, {
      "methods" : [ "DELETE" ],
      "pathPattern" : "/_/tenant"
    } ]
  } ],
  "permissionSets" : [ {
    "permissionName" : "module.erm-stripes.enabled",
    "displayName" : "OLF License",
    "description" : "Base property to enable any access the OLF License module",
    "visible" : false
  }, {
    "permissionName" : "mod-licenses.user",
    "displayName" : "OLF License - User",
    "description" : "A user of OLF License",
    "subPermissions" : [ "module.license-stripes.enabled" ],
    "visible" : true
  } ],
  "launchDescriptor" : {
    "dockerImage" : "folioorg/mod-licenses:1.0.2",
    "dockerPull" : true,
    "env" : [ {
      "name" : "JAVA_OPTIONS",
      "value" : "-Xmx256m"
    } ],
    "dockerArgs" : {
      "HostConfig" : {
        "PortBindings" : {
          "8080/tcp" : [ {
            "HostPort" : "%p"
          } ]
        }
      }
    }
  }
}