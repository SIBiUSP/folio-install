{
  "id" : "mod-audit-filter-0.0.3",
  "name" : "Audit Filter Module",
  "requires" : [ {
    "id" : "audit",
    "version" : "1.0"
  } ],
  "provides" : [ ],
  "filters" : [ {
    "methods" : [ "*" ],
    "pathPattern" : "/*",
    "phase" : "pre",
    "type" : "request-only",
    "modulePermissions" : [ "audit.item.post" ]
  }, {
    "methods" : [ "*" ],
    "pathPattern" : "/*",
    "phase" : "post",
    "type" : "request-only",
    "modulePermissions" : [ "audit.item.post" ]
  } ],
  "permissionSets" : [ ],
  "launchDescriptor" : {
    "dockerImage" : "folioorg/mod-audit-filter:0.0.3",
    "dockerPull" : true,
    "env" : [ {
      "name" : "JAVA_OPTIONS",
      "value" : "-Xmx256m"
    } ],
    "dockerArgs" : {
      "HostConfig" : {
        "PortBindings" : {
          "8081/tcp" : [ {
            "HostPort" : "%p"
          } ]
        }
      }
    }
  }
