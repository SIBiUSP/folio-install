{
  "id": "mod-data-loader-1.0.0",
  "name": "Data Loader Module",
  "provides": [
    {
      "id": "data-loader",
      "version": "1.0",
      "handlers": [
        {
          "methods": ["POST"],
          "pathPattern": "/load/marc-rules"
        },
        {
          "methods": ["GET"],
          "pathPattern": "/load/marc-rules"
        },
        {
          "methods": ["POST"],
          "pathPattern": "/load/marc-data"
        },
        {
          "methods": ["GET"],
          "pathPattern": "/load/marc-data"
        },
        {
          "methods": ["POST"],
          "pathPattern": "/load/marc-data/test"
        },
        {
          "methods": ["POST"],
          "pathPattern": "/load/static"
        },
        {
          "methods": ["GET"],
          "pathPattern": "/load/static"
        },
        {
          "methods": ["POST"],
          "pathPattern": "/load/static/test"
        }
      ] 
    } ],
  "requires": [
    {
      "id": "instance-storage",
      "version": "6.0"
    }
  ]
}